<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.AllAnalyticsPage">


    <ImageView
        android:id="@+id/backBtnAnalytics"
        android:elevation="8dp"
        android:layout_width="28dp"
        android:layout_height="28dp"
        android:background="@drawable/round_shape"
        android:src="@drawable/back_btn_new"
        android:layout_marginStart="12dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
         />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="40dp"
        android:gravity="center"
        android:layout_marginRight="8dp"
        android:orientation="vertical">

        <TextView
            style="@style/SpaceGroteskBold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="16sp"
            android:fontFamily="@font/space_grotesk_bold"
            android:text="Analytics"
            android:textColor="#393E59"
            android:textSize="24dp" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/myAnalyticsList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </LinearLayout>


</RelativeLayout>